<div id="messageLine" ng-cloak>
    <div id="GoodMessageWrapper" ng-show="displayGoodMessage">
        <div id="goodmessage">{{goodMessageText}}</div>
    </div>
    
    <div id="BadMessageWrapper" ng-show="displayBadMessage">
        <div id="badmessage">{{badMessageText}}</div>
    </div>
</div>
